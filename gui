local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("TITLE", "DarkTheme")

local Universal = Window:NewTab("Universal")
local lp = Window:NewTab("local player")

local universal = Universal:NewSection("Universal hacks")

universal:NewKeybind("UI toggle", "let's u select a keybind to toggle the UI", Enum.KeyCode.RightControl, function()
	Library:ToggleUI()
end)

universal:NewButton("aimbot", "Gives u aimbot lol", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Exunys/Aimbot-Script/main/Aimbot%20Script.lua",true))()
end)

universal:NewButton("Yield", "Gives u infinate yield", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source",true))()
end)

universal:NewButton("esp", "Enables esp", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/ic3w0lf22/Unnamed-ESP/master/UnnamedESP.lua',true))()
end)
